allowHTML: true
apply-presets: true
breaks: false
varsPreset: public
lang: en

docs-viewer:
  project-name: Telescope
  logo-title: Telescope
  show-revision: true
  themes: ['dark']
  github-url-prefix: https://github.com/iamtelescope/telescope/tree/main/docs

  index-page:
    hidden: true
    title: Telescope
    description: "Telescope docs"

connector:
    type: github
    github:
        endpoint: https://api.github.com
        owner: iamtelescope
        repo: telescope

vcs:
  remoteBase: https://github.com/iamtelescope/telescope/tree/main/docs
